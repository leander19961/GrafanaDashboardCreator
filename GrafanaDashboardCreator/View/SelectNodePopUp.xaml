<Window x:Class="GrafanaDashboardCreator.View.SelectNodePopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GrafanaDashboardCreator.View"
        mc:Ignorable="d"
        Title="SelectNodePopUp" Height="150" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="Select node for GET request" HorizontalAlignment="Center" Margin="0,0,0,75" VerticalAlignment="Center"/>
        <Button x:Name="SelectNode" Content="GET Request Resources From Node" HorizontalAlignment="Center" Margin="0,75,0,0" VerticalAlignment="Center" Click="RemoveDashboard_Click"/>
        <ComboBox x:Name="NodeSlectBox" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" Width="120"/>
    </Grid>
</Window>
