<Window x:Class="GrafanaDashboardCreator.View.ErrorPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GrafanaDashboardCreator.View"
        mc:Ignorable="d"
        Title="ErrorPopUp" Height="150" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Button Content="Okay" HorizontalAlignment="Center" Margin="0,0,0,20" VerticalAlignment="Bottom" Click="OkayButton_OnClick"/>
        <Label x:Name="ErrorMessageLabel" Content="ErrorMessage!" HorizontalAlignment="Center" Margin="0,22,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
